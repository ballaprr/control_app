# Production Environment Variables for React Frontend
# Copy this to frontend/.env.production and replace with your actual values

# Replace with your actual EC2 domain/IP
REACT_APP_API_BASE_URL=http://your-ec2-public-ip:8000/api
REACT_APP_ENVIRONMENT=production

# Optional: If you want to use HTTPS later
# REACT_APP_API_BASE_URL=https://your-domain.com/api

# Optional: Add other production-specific variables
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
